{
    "manifest_version": 3,
    "name": "Genie",
    "version": "1.0.0",
    "description": "Genie - Your Wish is Our Command",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "128": "./src/assets/icon128.png"
      }
    },
    "background": {
      "service_worker": "./src/background.js"
    },
    "web_accessible_resources": [{
      "matches": ["<all_urls>"],
      "resources": ["./src/assets/*"]
    }],
    "permissions": []
  }